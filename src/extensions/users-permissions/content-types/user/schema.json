{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "department": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department.department",
      "inversedBy": "users"
    },
    "teacherCourseAssignments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-assignment.course-assignment",
      "mappedBy": "teacher"
    },
    "studentCourseAssignments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-assignment.course-assignment",
      "inversedBy": "students"
    },
    "institute": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::institute.institute",
      "inversedBy": "users"
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance-student.attendance-student",
      "mappedBy": "student"
    },
    "assessmentSubmissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assessment-submission.assessment-submission",
      "mappedBy": "student"
    }
  }
}
